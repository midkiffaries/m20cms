		<copyright>{COPYRIGHT}</copyright>
		<link>{URL_LINK}</link>
		<language>en-us</language>

		<itunes:author>{TITLE}</itunes:author>
		<itunes:subtitle>{DESCRIPTION}</itunes:subtitle>
